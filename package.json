{
  "name": "json-schema-library",
  "version": "4.1.4",
  "description": "Customizable and hackable json-validator and json-schema utilities for traversal, data generation and validation",
  "main": "index.ts",
  "scripts": {
    "dist": "rm -rf dist; yarn dist:es5 && yarn dist:es6",
    "dist:es5": "NODE_ENV=production webpack",
    "dist:es6": "tsc --outDir distmodule --target ES2019",
    "test": "mocha --recursive -R dot 'test/**/*.test.js'",
    "test:integration": "mocha --recursive -R spec 'test/integration/**/*.test.js'",
    "test:unit": "mocha --recursive -R spec 'test/unit/**/*.test.js'",
    "test:spec": "mocha --recursive -R spec 'test/spec/**/*.test.js'",
    "tdd": "watch \"npm run test:unit\" lib/ test/",
    "coverage": "nyc npm run test --reporter=lcov",
    "analyze": "NODE_ENV=production webpack --json | webpack-bundle-size-analyzer",
    "lint": "eslint lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagold/json-schema-library.git"
  },
  "author": "Sascha Goldhofer <github@saschagoldhofer.de> (https://github.com/sagold/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sagold/json-schema-library/issues"
  },
  "homepage": "https://github.com/sagold/json-schema-library",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "chai": "^4.2.0",
    "chalk": "^2.3.0",
    "eslint": "^7.13.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "glob": "^7.1.2",
    "json-schema-test-suite": "json-schema-org/JSON-Schema-Test-Suite.git#master",
    "mocha": "^3.1.2",
    "nyc": "^10.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "watch": "^1.0.1",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "deepmerge": "^4.0.0",
    "gson-pointer": "^3.2.1",
    "gson-query": "^2.0.0",
    "valid-url": "^1.0.9"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  }
}
