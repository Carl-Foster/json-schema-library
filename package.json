{
  "name": "json-schema-library",
  "version": "1.1.0",
  "description": "Customizable and hackable json-validator and json-schema utilities for traversal, data generation and validation",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test-unit; npm run test-integration",
    "test-integration": "mocha --recursive 'test/integration/**/*.test.js' -R spec; exit 0",
    "test-unit": "mocha --recursive 'test/unit/**/*.test.js' -R spec; exit 0",
    "tdd": "watch 'npm run test-unit' lib/ test/; exit 0",
    "coverage": "nyc npm run test --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagold/json-schema-library.git"
  },
  "author": "Sascha Goldhofer <noreply@saschagoldhofer.de> (https://github.com/sagold/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sagold/json-schema-library/issues"
  },
  "homepage": "https://github.com/sagold/json-schema-library",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "mocha": "^3.1.2",
    "nyc": "^10.0.0",
    "watch": "^1.0.1"
  },
  "dependencies": {
    "ajv": "^4.8.2",
    "gson-pointer": "^1.0.4"
  },
  "publishConfig": {
      "registry": "http://registry.npmjs.org"
  }
}
